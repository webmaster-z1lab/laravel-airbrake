{
    "name": "kouz/laravel-airbrake",
    "description": "Laravel service provider for Airbrake https://github.com/airbrake/phpbrake",
    "keywords": ["airbrake", "laravel"],
    "require": {
        "php": ">=5.4",
        "airbrake/phpbrake": "^0.5.2",
        "illuminate/support": "5.*"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Theo Kouzelis",
            "email": "tkouzelis@outlook.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Kouz\\LaravelAirbrake\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Kouz\\LaravelAirbrake\\AirbrakeServiceProvider"
            ]
        }
    }
}
